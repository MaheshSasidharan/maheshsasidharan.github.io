@model IMAnnualReview.Domain.Entities.AppRole        

<div class="modal-content">       
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="myModalLabel">Role Details</h3>
</div>        
<div  class="modal-body">
<div class="display-label">RoleName</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.RoleName)
    </div>

    <div class="display-label">RoleAbbr</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.RoleAbbr)
    </div>

    <div class="display-label">ShortDesc</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ShortDesc)
    </div>

    <div class="display-label">LongDesc</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LongDesc)
    </div>

    <div class="display-label">Active</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Active)
    </div>

    <div class="display-label">UpdatedBy</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UpdatedBy)
    </div>

    <div class="display-label">DateUpdated</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DateUpdated)
    </div>

    <div class="display-label">CreatedBy</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreatedBy)
    </div>

    <div class="display-label">DateCreated</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DateCreated)
    </div>
    <div class="display-label">Users</div>
    <div class="display-field">
    <ul>
    @{
        foreach (var d in Model.AppUserRoles)
        {
            <li>@d.AppUser.UserId</li>
            }

    }
     </ul>
    </div>
</div>
</div>
