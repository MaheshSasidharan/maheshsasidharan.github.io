<?xml version="1.0"?>

<!-- Web.config appSettings section, for default MVC4 and CCOM Services -->

<appSettings>
  <!-- ****************************************************************** -->
  <!-- *** DEVELOPMENT *** -->
  <!-- ****************************************************************** -->
  <!-- set to datetime value in order to use a date for development/testing (ex: 01/01/2014)-->
  <add key="DevelopmentDate" value=""/>
  <!-- set to [DEV | TEST | PROD] value in order to override calculated deployment environment-->
  <add key="DeploymentOverride" value=""/>
  <!-- *********** -->
  <!-- this will disable the ITS Passport Authentication (the Authorization will still occur); -->
  <!-- NOTE: will throw Exception if environment is PROD -->
  <add key="ItsPassportIsDisabled" value="false"/>
  <add key="ItsPassportHealthcareOrHawkId" value="{HEALTHCAREORHAWKID}"/>
  <add key="ItsPassportUniversityId" value="{UNIVERSITYID}"/>
  <!-- ****************************************************************** -->
  <!-- ****************************************************************** -->
  <!-- *** DEFAULT: MVC4 *** -->
  <!-- ****************************************************************** -->
  <add key="webpages:Version" value="2.0.0.0" />
  <add key="webpages:Enabled" value="false" />
  <add key="PreserveLoginUrl" value="true" />
  <add key="ClientValidationEnabled" value="true" />
  <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  <!-- ****************************************************************** -->
  <!-- ****************************************************************** -->
  <!-- *** APPLICATION *** -->
  <!-- ****************************************************************** -->
  <add key="ApplicationName" value="IMAnnualReview"/>
  <add key="ApplicationVersion" value="1.0.0.0"/>
  <!-- ****************************************************************** -->
  <!-- ****************************************************************** -->
  <!-- *** DEPLOYMENT ***-->
  <!-- ****************************************************************** -->
  <add key="DevServers" value="MAM*;MU*;MEQ*;MHV*;MX*;HC-WEBDEV01"/>
  <add key="TestServers" value="HC-VWEBTEST01"/>
  <add key="ProdServers" value="HC-WEB01;HC-WEB02"/>
  <add key="StageServers" value="HC-WEBSTAGE01"/>
  <!--  -->
  <!-- SQL -->
  <add key="DevSqlServers" value="HC-SQL7DEV;HC-SQL5DEV"/>
  <add key="TestSqlServers" value="HC-SQL7TEST;HC-SQL5TEST"/>
  <add key="ProdSqlServers" value="HC-SQL7;HC-SQL5"/>
  <!-- ORA -->
  <add key="DevOraServers" value=""/>
  <add key="TestOraServers" value=""/>
  <add key="ProdOraServers" value=""/>
  <!-- ****************************************************************** -->
  <!-- ****************************************************************** -->
