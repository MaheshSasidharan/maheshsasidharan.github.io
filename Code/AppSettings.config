    <add key="EmailNotificationBccAddresses" value="{CUSTOM_CODE}"/>
   -->
  <!-- ****************************************************************** -->
  <!-- ****************************************************************** -->
  <!-- *** ERROR ***-->
  <!-- ****************************************************************** -->
  <add key="MainErrorMessage" value="There has been an error in the Application"/>
  <add key="ErrorMessage1" value="Please try again or contact Tech Support below"/>
  <add key="ErrorMessage2" value=""/>
  <add key="TechSupportContact" value="Tech Support"/>
  <add key="TechSupportEmail" value="mailto:myemail@healthcare.uiowa.edu"/>
  <add key="TechSupportPhone" value="319-111-2222"/>
